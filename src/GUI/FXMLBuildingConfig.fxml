<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="225.0" prefWidth="336.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.FXMLBuildingConfigController">
   <children>
      <Label alignment="CENTER" layoutX="68.0" layoutY="14.0" prefHeight="17.0" prefWidth="200.0" text="Config for new building">
         <font>
            <Font name="Avenir Next Bold" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="nameTextField" layoutX="133.0" layoutY="55.0" promptText="Type name" />
      <TextField fx:id="addressTextField" layoutX="133.0" layoutY="99.0" promptText="Type Address" />
      <Label layoutX="10.0" layoutY="60.0" prefHeight="19.0" prefWidth="112.0" text="Building name:">
         <font>
            <Font name="Avenir Next Bold" size="13.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="103.0" prefHeight="19.0" prefWidth="112.0" text="Building address:">
         <font>
            <Font name="Avenir Next Bold" size="13.0" />
         </font>
      </Label>
      <Button layoutX="59.0" layoutY="184.0" mnemonicParsing="false" onAction="#createBuilding" prefHeight="27.0" prefWidth="218.0" text="Create new building" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="14.0" layoutY="155.0" prefHeight="17.0" prefWidth="304.0">
